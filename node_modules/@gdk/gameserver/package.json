{
	"name": "@gdk/gameserver",
	"version": "3.6.4",
	"main": "./dist/server/index.js",
	"types": "./dist/server/index.d.ts",
	"description": "GDK Server",
	"bin": {
		"gameserver": "./bin/gameserver.js"
	},
	"repository": {
		"type": "git",
		"url": "https://mercure.lotsys.com/stash/projects/DGFGDK/repos/framework"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build:production": "npm run build",
		"build": "npm run clean && npm run build:server && npm run build:channel",
		"build:server": "tsc --project ./src/server/tsconfig.json",
		"build:channel": "webpack --mode production --config src/front/channel/webpack.config.js",
		"build:debug": "npm run build:server && npm run build:channel",
		"watch:server": "npm run build:server -- --watch",
		"watch:front": "webpack --mode production --config src/front/channel/webpack.config.js --watch",
		"watch": "npm run watch:server",
		"lint": "eslint ."
	},
	"dependencies": {
		"@falcon/bridge": "2.13.6",
		"@gdk/backutils": "3.6.4",
