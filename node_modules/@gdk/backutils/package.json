{
	"name": "@gdk/backutils",
	"version": "3.6.4",
	"description": "GDK shared utils for packer & gameserver",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://mercure.lotsys.com/stash/projects/DGFGDK/repos/framework"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build:production": "npm run build",
		"build": "npm run clean && npm run compile && npm run dts",
		"compile": "tsc --project ./tsconfig.json",
		"dts": "homemade-dts-bundler --entry ./dist/.declaration/index.d.ts --output ./dist/index.d.ts --libraryName @gdk/backutils",
		"watch": "npm run compile -- --watch",
		"test": "jest --config jest.config.json",
		"test:watch": "npm run test -- --watch",
		"test:coverage": "jest --config jest.config.json --coverage",
		"lint": "eslint ."
	},
	"dependencies": {
		"ajv": "^6.7.0",
		"fs-extra": "^9.0.1",
		"winston": "^3.3.4"
	},
	"engines": {
		"node": ">=10.15.0"
	},
	"author": "FGS-FR",
	"license": "UNLICENSED",
	"gitHead": "aa258e8c9e665d4590958db784990717111121e5"
}
