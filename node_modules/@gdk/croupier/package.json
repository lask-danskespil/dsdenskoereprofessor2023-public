{
	"name": "@gdk/croupier",
	"version": "3.6.4",
	"description": "GDK croupier tool",
	"main": "./dist/index.js",
	"types": "./dist/.declaration/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://mercure.lotsys.com/stash/projects/DGFGDK/repos/framework"
	},
	"bin": {
		"croupier": "./bin/cli.js"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build:production": "npm run build",
		"build": "npm run clean && npm run compile && npm run dts",
		"compile": "tsc --project ./tsconfig.json",
		"dts": "homemade-dts-bundler --entry ./dist/.declaration/index.d.ts --output ./dist/index.d.ts --libraryName @gdk/croupier",
		"watch": "npm run compile -- --watch",
		"test": "jest --runInBand --config jest.config.json",
		"test:watch": "npm run test -- --watch",
		"test:coverage": "jest --config jest.config.json --coverage",
		"lint": "eslint ."
	},
	"dependencies": {
		"@gdk/backutils": "3.6.4",
		"@gdk/core": "3.6.4",
		"alasql": "1.7.3",
		"chalk": "^3.0.0",
		"commander": "^9.3.0",
		"decompress": "^4.2.1",
		"exceljs": "3.3.1",
		"fs-extra": "^3.0.1",
		"inquirer": "^7.0.1",
		"lodash": "^4.17.11",
		"mocha": "10.0.0",
		"seedrandom": "^3.0.5",
		"xml-js": "^1.6.11"
	},
	"engines": {
		"node": ">=10.15.0"
	},
	"author": "FGS France",
	"license": "UNLICENSED",
	"gitHead": "aa258e8c9e665d4590958db784990717111121e5"
}
