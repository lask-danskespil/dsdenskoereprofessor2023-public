{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Settings",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"name": {
			"description": "The name of this settings.",
			"type": "string",
			"minLength": 1,
			"maxLength": 255
		},
		"gameExternalReference": {
			"description": "The external reference from the related game.",
			"type": "string",
			"pattern": "^[a-zA-Z0-9]*$",
			"minLength": 1,
			"maxLength": 255
		},
		"externalReference": {
			"description": "The external reference of this settings.",
			"type": "string",
			"pattern": "^[a-zA-Z0-9]*$",
			"minLength": 1,
			"maxLength": 255
		},
		"description": {
			"description": "The description of this settings.",
			"type": "string",
			"minLength": 1,
			"maxLength": 2048
		},
		"currency": {
			"description": "The currency used by this settings.",
			"type": "string",
			"pattern": "^[A-Z]*$",
			"minLength": 3,
			"maxLength": 3
		},
		"parameters": {
			"description": "The parameters provided by this settings (Trj,...)",
			"type": "object"
		}
	},
	"required": [
		"name",
		"externalReference",
		"description",
		"gameExternalReference",
		"parameters"
	]
}
