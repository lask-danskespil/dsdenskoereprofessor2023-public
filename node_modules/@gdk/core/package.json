{
	"name": "@gdk/core",
	"main": "./dist/core.bundle.js",
	"types": "./dist/index.d.ts",
	"version": "3.6.4",
	"description": "GDK Core",
	"repository": {
		"type": "git",
		"url": "https://mercure.lotsys.com/stash/projects/DGFGDK/repos/framework"
	},
	"gdkToRelease": [
		"/dist/core.bundle.min.js"
	],
	"scripts": {
		"clean": "rimraf dist",
		"build:production": "npm run build && npm run minimize",
		"build": "npm run clean && npm run webpack",
		"build:debug": "npm run clean && npm run webpack:debug",
		"webpack": "webpack --mode production --devtool source-map",
		"webpack:debug": "webpack --mode development",
		"minimize": "for i in dist/*.js; do terser --compress --mangle --comments /^!/ --output \"${i%.js}.min.js\" -- $i; done;",
		"watch": "npm run webpack:debug -- --watch",
		"test": "jest --config jest.config.json",
		"test:watch": "npm run test -- --watch",
		"test:coverage": "jest --config jest.config.json --coverage",
		"lint": "eslint ."
	},
	"dependencies": {
		"howler": "2.0.9",
		"i18next": "10.6.0"
	},
	"author": "Lotsys",
	"license": "UNLICENSED",
	"gitHead": "aa258e8c9e665d4590958db784990717111121e5"
}
